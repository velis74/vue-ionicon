{
  "name": "vue-cached-icon",
  "version": "2.1.1",
  "description": "Caching asynchronous icons loader for Vue",
  "author": "Jure Erzno≈ænik",
  "scripts": {
    "dev": "npm run dev --workspace=demo",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "test": "vitest run --coverage",
    "lint": "eslint ."
  },
  "main": "dist/vue-cached-icon.umd.js",
  "types": "dist/vue-cached-icon.vue.d.ts",
  "exports": {
    ".": {
      "require": "./dist/vue-cached-icon.umd.js",
      "import": "./dist/vue-cached-icon.mjs"
    }
  },
  "files": [
    "dist/*"
  ],
  "workspaces": ["demo"],
  "keywords": [
    "vue ionicons",
    "vue svg",
    "ionicons"
  ],
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git@github.com:velis74/vue-cached-icon.git"
  },
  "peerDependencies": {
    "axios": "^1.1.3",
    "isomorphic-dompurify": "^2.28.0",
    "vue": "^3.2.43"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@vitejs/plugin-vue": "^5",
    "@vitest/coverage-v8": "^3",
    "@vue/test-utils": "^2.2.4",
    "@vue/tsconfig": "^0.8.1",
    "eslint-config-velis": "^1.1.16",
    "jsdom": "^26",
    "rollup-plugin-visualizer": "^5.14.0",
    "typescript": "^5",
    "vite": "^5",
    "vite-plugin-dts": "^4",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^3",
    "vue-tsc": "^2"
  },
  "eslintConfig": {
    "extends": [
      "velis"
    ],
    "parserOptions": {
      "project": "./tsconfig.eslint.json"
    }
  }
}
